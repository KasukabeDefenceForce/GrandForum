<?xml version="1.0" encoding="UTF-8" ?>
<Report name="PNI Evaluation" reportType="RP_EVAL_RESEARCHER" pdfType="" pdfFiles="" pdfAllProjects="true" ajax="true" disabled="false">
    <Permissions>
        <Role start="REPORTING_CYCLE_START" end="REPORTING_CYCLE_END" role="PNI">
            <SectionPermission id="materials" permissions="rw" />
            <SectionPermission id="budget" permissions="rw" />
            <SectionPermission id="reviews" permissions="rw" />
        </Role>
    </Permissions>

    <!-- PNI Materials -->
    <ReportSection id="materials" name="PNI Materials" tooltip="" blobSection="SEC_NONE" type="ReportSection" renderpdf="false" private="true">
        <Instructions>
            <![CDATA[
               <p>Instructions needed</p>
            ]]>
        </Instructions>
        <ReportItem id="pni_materials_head" type="StaticReportItem">
            <![CDATA[<table>]]>
        </ReportItem>
        <ReportItemSet id="pni_materials" type="PNIMaterialsReportItemSet">
            <ReportItem id="pni_pdf" type="SubPDFReportItem" reportType="NIReport">
                <![CDATA[<tr>{$item}</tr>]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="pni_materials_tail" type="StaticReportItem">
            <![CDATA[</table>]]>
        </ReportItem>
        
    </ReportSection>

    <!-- PNI Budget -->
    <ReportSection id="budget" name="PNI Budget" tooltip="" blobSection="SEC_NONE" type="ReportSection" renderpdf="false" private="false">
        <Instructions>
            <![CDATA[
               <p>Instructions needed</p>
            ]]>
        </Instructions>
        <ReportItem id="pni_budget_head" type="StaticReportItem">
            <![CDATA[
            <script type="text/javascript">
                $(document).ready(function(){ 
                    $('.pni_budget_accordions').accordion({active: false, collapsible: true, icons: false }); 
                })
            </script>
            ]]>
        </ReportItem>
        <ReportItemSet id="pni_materials" type="PNIMaterialsReportItemSet">
            <ReportItem id="pni_budget" blobType="BLOB_EXCEL" blobItem="RES_BUD_BUDGET" type="EvalBudgetReportItem" />
        </ReportItemSet>
    </ReportSection>

    <!-- PNI Reviews -->
    <ReportSection id="reviews" name="PNI Reviews" tooltip="" blobSection="SEC_NONE" type="EditableReportSection" renderpdf="false" private="true">
        <Instructions>
            <![CDATA[
               <p>Instructions needed</p>
            ]]>
        </Instructions>
        <ReportItem id="pni_review_head" type="StaticReportItem">
            <![CDATA[
            <script type="text/javascript">
                $(document).ready(function(){ 
                    $('.ni_review_item_wrapper').accordion({active: false, collapsible: true, icons: false, header: "h2" }); 
                })
            </script>
            ]]>
        </ReportItem>
        <ReportItemSet id="pni_materials" type="PNIMaterialsReportItemSet">
            <ReportItemSet id="pni_materials" type="NIReviewReportItemSet">
                <ReportItem id="RI1" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_EXCELLENCE" blobSubItem="0">
                <![CDATA[
                <h3>1. Excellence of the Research Program</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI2" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_HQPDEVELOPMENT" blobSubItem="0">
                <![CDATA[
                <h3>2. Development of HQP</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI3" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_NETWORKING" blobSubItem="0">
                <![CDATA[
                <h3>3. Networking and Partnerships</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI4" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_KNOWLEDGE" blobSubItem="0">
                <![CDATA[
                <h3>4. Knowledge and Technology Exchange and Exploitation</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI5" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_MANAGEMENT" blobSubItem="0">
                <![CDATA[
                <h3>5. Management of the Network</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI6" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_OVERALLSCORE" blobSubItem="0">
                <![CDATA[
                <h3>6. Overall Score</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI7" type="EvalReviewSelectReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_STOCKCOMMENTS" blobSubItem="0">
                <![CDATA[
                <h3>7. Stock Comments</h3>
                {$item}
                ]]>
                </ReportItem>
                <ReportItem id="RI8" type="EvalReviewTextareaReportItem" rows="6" width="100%" limit="600" blobType="BLOB_TEXT" blobItem="EVL_OTHERCOMMENTS" blobSubItem="0">
                <![CDATA[
                <h3>8. Evaluator Comments</h3>
                <div style="padding-left:16px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI9" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_REPORTQUALITY" blobSubItem="0">
                <![CDATA[
                <h3>9. Rating for Quality of Report</h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
                <ReportItem id="RI10" type="EvalReviewRadioReportItem" options="Exceptional|Good|Satisfactory|Unsatisfactory" orientation="horizontal" blobType="BLOB_ARRAY" blobItem="EVL_CONFIDENCE" blobSubItem="0">
                <![CDATA[
                <h3>10. Confidence Level of Evaluator </h3>
                <div style="padding-left:20px;">{$item}</div>
                ]]>
                </ReportItem>
            </ReportItemSet>
        </ReportItemSet>
    </ReportSection>

</Report>
