        </ReportItem>
        <ReportItem id="report_info_foot" type="StaticReportItem">
            <![CDATA[
                <br /><small><b>Generated:</b> {$timestamp}</small></div>
                </div>
            ]]>
        </ReportItem>
    </ReportSection>
    <ReportSection id="isac_comments" name="ISAC Comments" tooltip="" blobSection="SEC_NONE" type="ReportSection" renderpdf="true" private="true" pagebreak="false">
        <ReportItemSet type="ProjectISACReportItemSet">
            <ReportItem type="SubBookmarkReportItem" text="{$user_reversed_name}">
                <![CDATA[
                   <h2>{$user_reversed_name}</h2>
                   <p>{$isac_comment}</p>
                ]]>
            </ReportItem>
