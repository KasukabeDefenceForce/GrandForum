        </If>
        <ReportItem id="section7_upload" type="Upload" blobItem="SECTION7_UPLOAD" blobType="BLOB_RAW" rich="true" fileSize="5">
            <![CDATA[
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="section8" type="Textarea" blobItem="SECTION8" rich="true" limit="550">
