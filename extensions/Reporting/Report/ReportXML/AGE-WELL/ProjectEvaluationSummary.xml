<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Project Evaluation" reportType="RP_PROJ_EVALUATION_SUMMARY" pdfType="RPTP_PROJ_EVALUATION_SUMMARY" ajax="true">
    <Permissions>
        <Role role="STAFF+">
            <SectionPermission id="summary" permissions="rw" />
            <SectionPermission id="overview" permissions="rw" />
        </Role>
    </Permissions>
    <ReportSection id="summary" type="ReportSection" tooltip="Summary" name="Summary">
        <ReportItem id="head" type="Static">
            <![CDATA[
                <script type="text/javascript">
                    var tableToExcel = (function () {
                        var uri = 'data:application/vnd.ms-excel;base64,',
                            template = '<html xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns="http://www.w3.org/TR/REC-html40"><head><!--[if gte mso 9]><xml><x:ExcelWorkbook><x:ExcelWorksheets><x:ExcelWorksheet><x:Name>{worksheet}</x:Name><x:WorksheetOptions><x:DisplayGridlines/></x:WorksheetOptions></x:ExcelWorksheet></x:ExcelWorksheets></x:ExcelWorkbook></xml><![endif]--></head><body><table>{table}</table></body></html>',
                            base64 = function (s) {
                                return window.btoa(unescape(encodeURIComponent(s)))
                            }, format = function (s, c) {
                                return s.replace(/{(\w+)}/g, function (m, p) {
                                    return c[p];
                                })
                            }
                        return function (table, name, filename) {
                            if (!table.nodeType) table = document.getElementById(table)
                            var ctx = {
                                worksheet: name || 'Worksheet',
                                table: table.innerHTML
                            }
                       document.getElementById("dlink").href = uri + base64(format(template, ctx));
                                document.getElementById("dlink").download = filename;
                                document.getElementById("dlink").click();
                        }
                    })();
                </script>
                <a id="dlink"  style="display:none;"></a>
                <input id="btnExport" type="button" onclick="tableToExcel('summary', 'Project Evaluation Summary', 'ProjectEvaluationSummary.xls')" value="Download as Spreadsheet" />
                <table id="summary" class='wikitable small'>
                    <thead>
                        <tr>
                            <th></th>
                            <th colspan='3'>Scientific Excellence</th>
                            <th colspan='7'>Networking and Partnerships</th>
                            <th colspan='3'>KTEE (KM & Commercialization combined)</th>
                            <th colspan='5'>HQP</th>
                        </tr>
                        <tr>
                            <th></th>
                            <th># invited expert presentations meetings keynotes, etc. at national or international-level academic conferences and events</th>
                            <th># journal publications</th>
                            <th># of international collaborators involved in the project</th>
                            <th># of partners</th>
                            <th>Total cash and in-kind contributions the project received</th>
                            <th># of stakeholders (older adult or caregiver)</th>
                            <th># of stakeholders (community organization)</th>
                            <th># of stakeholders (industry member)</th>
                            <th># of stakeholders (policymaker)</th>
                            <th># of project meetings and events (workshops) that included non-academic partners</th>
                            <th># of non-academic dissemination activities (total)</th>
                            <th># of licensing agreements</th>
                            <th># of patents</th>
                            <th># of HQP hired on project</th>
                            <th># of HQP involved as a lead author on a publication</th>
                            <th># of HQP that worked directly with partner organizations</th>
                            <th># of HQP successfully completing EPIC training program</th>
                            <th># of HQP involved as a lead author on a publication</th>
                        </tr>
                    </thead>
                    <tbody>
            ]]>
        </ReportItem>
        <ReportItemSet id="projects" type="AllProjectsReportItemSet">
            <ReportItem id="row" type="Static">
                <![CDATA[
                    <tr>
                        <td>{$project_name}</td>
