                        <td align="right">{getText(RP_PROJ_EVALUATION,HQP,REV_1_6_M9A,0,0,{$project_id})}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="foot" type="Static">
            <![CDATA[
                    </tbody>
                </table>
            ]]>
        </ReportItem>
