<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Project Evaluation" reportType="RP_PROJ_EVALUATION_SUMMARY" pdfType="RPTP_PROJ_EVALUATION_SUMMARY" ajax="true">
    <Permissions>
        <Role role="STAFF+">
            <SectionPermission id="summary" permissions="rw" />
        </Role>
    </Permissions>
    <ReportSection id="overview" type="ReportSection" tooltip="Summary" name="Summary">
        <ReportItem id="head" type="Static">
            <![CDATA[
                <table class='wikitable small'>
                    <thead>
                        <tr>
                            <th></th>
                            <th colspan='3'>Scientific Excellence</th>
                            <th colspan='4'>Networking and Partnerships</th>
                            <th colspan='3'>KTEE (KM & Commercialization combined)</th>
                            <th colspan='4'>HQP</th>
                        </tr>
                        <tr>
                            <th></th>
                            <th># invited expert presentations meetings keynotes, etc. at national or international-level academic conferences and events</th>
                            <th># journal publications</th>
                            <th># of international collaborators involved in the project</th>
                            <th># of partners</th>
                            <th>Total cash and in-kind contributions the project received</th>
                            <th># of stakeholders involved in research</th>
                            <th># of project meetings and events (workshops) that included non-academic partners</th>
                            <th># of non-academic dissemination activities (total)</th>
                            <th># of licensing agreements</th>
                            <th># of patents</th>
                            <th># of HQP hired on project</th>
                            <th># of HQP involved as a lead author on a publication</th>
                            <th># of HQP that worked directly with partner organizations</th>
                            <th># of HQP successfully completing EPIC training program</th>
                        </tr>
                    </thead>
                    <tbody>
            ]]>
        </ReportItem>
        <ReportItemSet id="projects" type="AllProjectsReportItemSet">
            <ReportItem id="row" type="Static">
                <![CDATA[
                    <tr>
                        <td>{$project_name}</td>
                        <td>{getText(RP_PROJ_EVALUATION,RESEARCH,REV_1_5_M2,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,RESEARCH,REV_2_5_M1,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,RESEARCH,REV_1_5_M1,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,NETWORKING,REV_1_5_M1,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,NETWORKING,REV_1_5_M3,0,0,{$project_id})}</td>
                        <td>&nbsp;</td>
                        <td>{getText(RP_PROJ_EVALUATION,NETWORKING,REV_2_5_M1,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,KTEE1,REV_3_5_M1A,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,KTEE2,REV_1_5_M2,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,KTEE2,REV_2_5_M4,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,HQP,REV_1_6_M1,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,HQP,REV_1_6_M9A,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,HQP,REV_1_6_M4,0,0,{$project_id})}</td>
                        <td>{getText(RP_PROJ_EVALUATION,HQP,REV_1_6_M5,0,0,{$project_id})}</td>
                    </tr>
                ]]>
            </ReportItem>
        </ReportItemSet>
        <ReportItem id="head" type="Static">
            <![CDATA[
                    </tbody>
                </table>
            ]]>
        </ReportItem>
    </ReportSection>
</Report>
