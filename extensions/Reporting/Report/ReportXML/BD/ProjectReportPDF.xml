<?xml version="1.0" encoding="UTF-8" ?>
<Report name="Project Report" reportType="RP_PROJECT_REPORT_{GET(id)}" pdfType="RP_PROJECT_REPORT_{GET(id)}" year="0" personId="0" ajax="true" disabled="false">
    <ReportSection id="report" type="NextPrevReportSection" name="Project Report" title="{$project_full_name} ({$project_name})&lt;br /&gt;{replace(_,:,{GET(id)})} Report" pagebreak="false">
        <!-- Start the Report -->
        <ReportItem id="description" type="Textarea" blobItem="DESCRIPTION" rich="true">
            <![CDATA[
                <h3>1. Description of work and next steps</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="milestones" type="Textarea" blobItem="MILESTONES" rich="true">
            <![CDATA[
                <h3>2. Activities and milestones</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="edi" type="Textarea" blobItem="EDI" rich="true">
            <![CDATA[
                <h3>3. Equity, diversity, and inclusion update</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="challenges" type="Textarea" blobItem="CHALLENGES" rich="true">
            <![CDATA[
                <h3>4. Identified or anticipated challenges (if any)</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="adjustments" type="Textarea" blobItem="ADJUSTMENTS" rich="true">
            <![CDATA[
                <h3>5. Milestones adjustments</h3>
                {$item}
            ]]>
        </ReportItem>
        <ReportItem id="nextsteps" type="Textarea" blobItem="NEXTSTEPS" rich="true">
            <![CDATA[
                <h3>6. Next steps</h3>
                {$item}
            ]]>
        </ReportItem>
    </ReportSection>
</Report>
